#weui-cell{
    font-size: 38rpx;
}
.imageItem{
    margin-top: 20rpx;
    width: 100%;
    height: auto;
    padding: 12rpx;
    box-sizing: border-box;
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    justify-content: flex-start;
}
.chooseImage, .q-image{
    width: 200rpx;
    height: 200rpx;
    margin-left: 4rpx;
}
.q-image-remover{
    width: 36rpx;
    height: 36rpx;
    position: relative;
    bottom: 180rpx;
    right: 12rpx;
    z-index: 1;
}
#weui-btn{
    position: relative;
    top: 200rpx;
    width: 98%;
    background-color: #5599FE;
}
